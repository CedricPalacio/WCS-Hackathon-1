const axios = require("axios");

const options = {
  method: "GET",
  url: "https://api.foursquare.com/v3/places/search?ll=43.6043%2C%201.4437&radius=5000&categories=13065&sort=RATING&limit=10",
  headers: { accept: "application/json" },
};

axios
  .request(options)
  .then(function (response) {
    console.log(response.data);
  })
  .catch(function (error) {
    console.error(error);
  });
